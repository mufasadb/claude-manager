# Claude Manager User Environment Variables
# Copy this file to ~/.claude-manager/user.env and fill in your actual values

# Version Control & Cloud
GITHUB_TOKEN=ghp_your_github_personal_access_token_here
GITLAB_TOKEN=glpat-your_gitlab_personal_access_token
GITLAB_URL=https://gitlab.com

# AI & Language Models
OPENAI_API_KEY=sk-your_openai_api_key_here
GEMINI_API_KEY=your_google_gemini_api_key_here
OPENROUTER_API_KEY=sk-or-your_openrouter_api_key

# Databases & Backend Services
SUPABASE_PROJECT_REF=your_supabase_project_reference_id
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anonymous_key
DATABASE_URL=postgresql://user:password@localhost:5432/database

# Communication & Productivity
SLACK_BOT_TOKEN=xoxb-your_slack_bot_token_here
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url
NOTION_API_KEY=secret_your_notion_integration_token_here

# Web Scraping & Search
BRAVE_API_KEY=your_brave_search_api_key
BRIGHTDATA_API_TOKEN=your_bright_data_api_token
FIRECRAWL_API_KEY=your_firecrawl_api_key

# Audio & Media
ELEVENLABS_API_KEY=your_elevenlabs_api_key_for_tts

# Cloud Infrastructure
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1
S3_BUCKET=your-s3-bucket-name

# Email & Notifications
EMAIL_ADDRESS=your.email@example.com
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_email_username
SMTP_PASS=your_email_password

# Custom Endpoints & Local Services (Centralized Configuration)
# These URLs are now managed by the centralized service configuration system
# and can be overridden at multiple levels (process.env → user.env → project.env → defaults)
OLLAMA_SERVICE_URL=http://100.83.40.11:11434
TTS_SERVICE_URL=http://100.83.40.11:8080

# Advanced Service Configuration (Optional)
OLLAMA_TIMEOUT=60000
OLLAMA_MAX_RETRIES=3
OLLAMA_RETRY_DELAY=1000
TTS_TIMEOUT=30000
TTS_ENDPOINT=http://localhost:8080/tts
WEBHOOK_URL=https://your-webhook-endpoint.com